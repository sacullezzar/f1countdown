{
  "name": "f1countdown",
  "version": "1.0.0",
  "description": "F1 Countdown App",
  "private": true,
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "webpack --mode production",
    "start": "npm run build && node server.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sacullezzar/f1countdown.git"
  },
  "author": "Sacullezzar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sacullezzar/f1countdown/issues"
  },
  "homepage": "https://github.com/sacullezzar/f1countdown#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@emotion/core": "^10.0.35",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.6",
    "express": "^4.17.3",
    "google-map-react": "^2.0.8",
    "isomorphic-fetch": "^3.0.0",
    "leaflet": "^1.6.0",
    "mappa-mundi": "0.0.4",
    "minimist": "^1.2.5",
    "node-forge": "^0.10.0",
    "p5": "^1.1.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.7.0",
    "react-p5-wrapper": "^2.3.0",
    "react-portal-overlay": "^1.0.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^4.4.1",
    "webfontloader": "^1.6.28"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "flow": "^0.2.3",
    "heroku": "^7.44.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "less": "^3.12.2",
    "less-loader": "^4.1.0",
    "prop-types": "15.6.2",
    "react-test-renderer": "^16.13.1",
    "sinon": "^7.5.0",
    "style-loader": "^0.23.1",
    "utils": "^0.3.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "minimist": "1.2.5"
  }
