{
  "name": "f1countdown",
  "version": "1.0.0",
  "description": "F1 Countdown App",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node server.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sacullezzar/f1countdown.git"
  },
  "author": "Sacullezzar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sacullezzar/f1countdown/issues"
  },
  "homepage": "https://github.com/sacullezzar/f1countdown#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.6",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.2.0",
    "webfontloader": "^1.6.28",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "flow": "^0.2.3",
    "heroku": "^7.24.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "react-test-renderer": "^16.8.4",
    "sinon": "^7.3.0",
    "style-loader": "^0.23.1"
  }
}
